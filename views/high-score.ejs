<style>
    .container-high-score, .back-button {
        margin-right: auto;
        margin-left: auto;
    }
    h2 {
        text-align: center;   
    }
    table {
        border-collapse: separate; /* Memisahkan batas sel */
        border-spacing: 5px;
        width: 50%;
        text-align: center;
        margin-right: auto;
        margin-left: auto;
    }

    @media only screen and (max-width: 600px) {
    table {
        width: 100%;
    }
}
    th, td {
        padding: 8px;
        text-align: center;
        color: gold;
        background-color: rgba(0, 0, 0, 0.5);
    }
    th {
        color: black;
    }
    th {
        background-color: gold;
    }
    .back-button {
        text-align: center;
    }
</style>

<div class="container-high-score">
    <h2>Skor Tertinggi</h2>
    <table>
        <tr>
            <th>Nama</th>
            <th>Skor</th>
        </tr>
        <% ScoreSort.forEach(function(e) { %>
        <tr>
            <td><%= e.nama %></td>
            <td> <%= e.score %></td>
        </tr>
        <% }); %>
    </table>
    <div class="back-button">
        <a href="/" >
            <button>
                Kembali
            </button>
        </a>
    </div>
   
</div>